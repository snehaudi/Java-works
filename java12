package lesson11;

public class My12thClass {

	public static void main(String[] args) {
		
		int[] myintarray = {1,2,3,4,5};
		System.out.println(myintarray[3]); //select 3rd index
		
		System.out.println("-------------------------");
		
		int index = 0;
		while (index < 5) {
		System.out.println(myintarray[index]);
		index++;
		}
		
	}

}
